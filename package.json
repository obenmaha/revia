{
  "name": "revia",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "smoke:test": "node scripts/smoke-test-simple.js",
    "smoke:preprod": "PREPROD_URL=https://app-kine-preview.vercel.app node scripts/smoke-test-simple.js",
    "smoke:docker": "node scripts/smoke-test-docker.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types:generate": "node scripts/generate-supabase-types.js",
    "init:supabase": "node scripts/init-supabase.js",
    "docker:build": "docker build -t app-kine:latest .",
    "docker:run": "docker run -p 8080:80 -e SUPABASE_URL=$env:SUPABASE_URL -e SUPABASE_ANON_KEY=$env:SUPABASE_ANON_KEY app-kine:latest",
    "docker:run:dev": "docker run -p 8080:80 -e SUPABASE_URL=https://your-project.supabase.co -e SUPABASE_ANON_KEY=your-anon-key app-kine:latest",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=app-kine:latest)",
    "docker:clean": "docker rmi app-kine:latest",
    "docker:logs": "docker logs $(docker ps -q --filter ancestor=app-kine:latest)",
    "brownfield:verify": "powershell -ExecutionPolicy Bypass -File run-brownfield-verification.ps1",
    "brownfield:verify:verbose": "powershell -ExecutionPolicy Bypass -File run-brownfield-verification.ps1 -Verbose",
    "brownfield:verify:no-build": "powershell -ExecutionPolicy Bypass -File run-brownfield-verification.ps1 -SkipBuild",
    "brownfield:verify:no-tests": "powershell -ExecutionPolicy Bypass -File run-brownfield-verification.ps1 -SkipTests",
    "scan:secrets": "npx gitleaks detect --no-git -v || echo 'Aucun secret détecté'",
    "scan:secrets:staged": "npx gitleaks detect --staged -v || echo 'Aucun secret détecté dans les fichiers staged'",
    "qa:analyze-bundle": "node scripts/analyze-bundle.mjs",
    "qa:fix-tests": "node scripts/fix-tests.mjs",
    "qa:optimize-imports": "node scripts/optimize-imports.mjs",
    "qa:validate-fixes": "node scripts/validate-fixes.mjs",
    "qa:all": "npm run qa:fix-tests && npm run qa:optimize-imports && npm run qa:analyze-bundle && npm run qa:validate-fixes",
    "prepare": "husky install"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
